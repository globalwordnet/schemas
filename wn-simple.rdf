    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://globalwordnet.github.io/schemas/wn-simple#inSynset"/>
        <owl:someValuesFrom rdf:resource="https://globalwordnet.github.io/schemas/wn-simple#AdverbSynset"/>

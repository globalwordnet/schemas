    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://globalwordnet.github.io/schemas/wn-simple#NounSynset">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en-us">A synset including noun word senses.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://globalwordnet.github.io/schemas/wn-simple#Synset"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://globalwordnet.github.io/schemas/wn-simple#containsWordSense"/>

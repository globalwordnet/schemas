                        "minItems": 1,
                        "items": {
                            "type": "object",
                            "required": ["@id","lemma","partOfSpeech"],
                            "additionalProperties": false,
                            "properties": {
                                "@id": { "type": "string" },
                                "lemma": {
                                   "type": "object",
                                   "required": ["writtenForm"],
                                   "additionalProperties": false,
                                   "properties": {
                                      "writtenForm": { "type": "string" },
                                      "tag": {
                                          "type": "array",
                                          "minItems": 1,
                                          "items": {
                                              "type": "object",
                                              "required": ["category","value"],
                                              "additionalProperties": false,
                                              "properties": {
                                                  "category" : { "type": "string" },
                                                  "value" : { "type": "string" }
                                              }
                                          }
                                      }
                                   }
                                },
                                "form": {
                                    "type": "array",
                                    "minItems": 1,

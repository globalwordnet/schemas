                    "language": { "language": "string" },
                    "email": { "type": "string" },
                    "license": { "type": "string" },
                    "version": { "type": "string" },
                    "citation": { "type": "string" }, 
                    "url": { "type": "string" }, 
                    "status": { "type": "string" }, 

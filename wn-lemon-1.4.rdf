    <rdf:label xml:lang="en">count</rdf:label>
  </owl:Class>
  <owl:Class rdf:about="https://globalwordnet.github.io/schemas/wn#Definition">
    <rdfs:label xml:lang="en">definition</rdfs:label>
  </owl:Class>

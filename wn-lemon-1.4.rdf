    <rdfs:label xml:lang="en">ILI definition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://globalwordnet.github.io/schemas/wn#PartOfSpeech">
    <rdfs:comment xml:lang="en">The syntactic class of the entry, e.g., noun, verb</rdfs:comment>
    <rdfs:label xml:lang="en">part of speech</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#PartOfSpeech"/>
    <owl:oneOf>
      <rdf:Description>
        <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#noun"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#verb"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#adjective"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#adverb"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#adjective_satellite"/>
                        <rdf:rest>
                          <rdf:Description>
                            <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#named_entity"/>
                            <rdf:rest>
                              <rdf:Description>
                                <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#conjunction"/>
                                <rdf:rest>
                                  <rdf:Description>
                                    <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#adposition"/>
                                    <rdf:rest>
                                      <rdf:Description>
                                        <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#other_pos"/>
                                        <rdf:rest>
                                          <rdf:Description>
                                            <rdf:first rdf:resource="https://globalwordnet.github.io/schemas/wn#unknown_pos"/>
                                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                          </rdf:Description>
                                        </rdf:rest>
                                      </rdf:Description>
                                    </rdf:rest>
                                  </rdf:Description>
                                </rdf:rest>
                              </rdf:Description>
                            </rdf:rest>
                          </rdf:Description>
                        </rdf:rest>
                      </rdf:Description>
                    </rdf:rest>
                  </rdf:Description>
                </rdf:rest>
              </rdf:Description>
            </rdf:rest>
          </rdf:Description>
        </rdf:rest>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>
  <owl:Class rdf:about="https://globalwordnet.github.io/schemas/wn#SenseRelType">
    <rdfs:label xml:lang="en">sense relation type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://globalwordnet.github.io/schemas/wn#SynsetRelType">
    <rdfs:label xml:lang="en">synset relation type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://globalwordnet.github.io/schemas/wn#Tag">
    <rdfs:comment xml:lang="en">The tag describing the category of a form</rdfs:comment>
    <rdfs:label xml:lang="en">tag</rdfs:label>
  </owl:Class>
  <PartOfSpeech rdf:about="https://globalwordnet.github.io/schemas/wn#adjective">
    <rdfs:label xml:lang="en">adjective</rdfs:label>
    <owl:sameAs rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#adjective"/>
  </PartOfSpeech>
  <PartOfSpeech rdf:about="https://globalwordnet.github.io/schemas/wn#adjective_satellite">
    <rdfs:label xml:lang="en">adjective satellite</rdfs:label>
  </PartOfSpeech>
  <PartOfSpeech rdf:about="https://globalwordnet.github.io/schemas/wn#adposition">
    <rdfs:label xml:lang="en">adposition</rdfs:label>
    <owl:sameAs rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#adposition"/>
  </PartOfSpeech>
  <PartOfSpeech rdf:about="https://globalwordnet.github.io/schemas/wn#adverb">
    <rdfs:label xml:lang="en">adverb</rdfs:label>
    <owl:sameAs rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#adverb"/>
  </PartOfSpeech>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#agent">
    <rdfs:comment xml:lang="en">A concept which is typically the one/that who/which does the action denoted by a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">agent</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#involved_agent"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#also">
    <rdf:type rdf:resource="https://globalwordnet.github.io/schemas/wn#SenseRelType"/>
    <rdfs:comment xml:lang="en">a word having a loose semantic relation to another word</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetと相手synsetとの間に何らかの関連がある</rdfs:comment>
    <rdfs:label xml:lang="en">also</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#also"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#anto_converse">
    <rdfs:comment xml:lang="en">word pairs that name or describe a single relationship from opposite perspectives</rdfs:comment>
    <rdfs:label xml:lang="en">converse antonym</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#anto_converse"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#anto_gradable">
    <rdfs:comment xml:lang="en">word pairs whose meanings are opposite and which lie on a continuous spectrum</rdfs:comment>
    <rdfs:label xml:lang="en">gradable antonym</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#anto_gradable"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#anto_simple">
    <rdfs:comment xml:lang="en">word pairs whose meanings are opposite but whose meanings do not lie on a continuous spectrum</rdfs:comment>
    <rdfs:label xml:lang="en">simple antonym</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#anto_simple"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#antonym">
    <rdf:type rdf:resource="https://globalwordnet.github.io/schemas/wn#SenseRelType"/>
    <rdfs:comment xml:lang="en">An opposite and inherently incompatible word</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該語義やsynsetの意味や程度が反対または対の関係となっている語義やsynset</rdfs:comment>
    <rdfs:label xml:lang="en">antonym</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#antonym"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#attribute">
    <rdfs:comment xml:lang="ja">(a=形容詞のsynsetから見て)当該synsetが相手synsetという属性を表す際に使われる
(n=名詞のsynsetから見て)当該synsetという属性を表す際に相手synsetを使う</rdfs:comment>
    <rdfs:comment xml:lang="en">an abstraction belonging to or characteristic of an entity</rdfs:comment>
    <rdfs:label xml:lang="en">attribute</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#attribute"/>
  </SynsetRelType>
  <owl:DatatypeProperty rdf:about="https://globalwordnet.github.io/schemas/wn#audio">
    <rdfs:comment xml:lang="en">Links to an audio file for a pronunciation</rdfs:comment>
    <rdfs:domain rdf:resource="https://globalwordnet.github.io/schemas/wn#Pronunciation"/>
    <rdfs:label xml:lang="en">audio</rdfs:label>
  </owl:DatatypeProperty>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#augmentative">
    <rdfs:comment xml:lang="en">A concept used to refer to generally larger members of a class</rdfs:comment>
    <rdfs:label xml:lang="en">augmentative</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#has_augmentative"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#be_in_state">
    <rdfs:comment xml:lang="en">X is qualified by Y</rdfs:comment>
    <rdfs:label xml:lang="en">be in state</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#state_of"/>
  </SynsetRelType>
  <owl:DatatypeProperty rdf:about="https://globalwordnet.github.io/schemas/wn#category">
    <rdfs:comment xml:lang="en">The data category of a tag</rdfs:comment>
    <rdfs:domain rdf:resource="https://globalwordnet.github.io/schemas/wn#Tag"/>
    <rdfs:label xml:lang="en">category</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#causes">
    <rdfs:comment xml:lang="en">Concept A is an entity that produces an effect or is responsible for events or
results of Concept B.</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetを行うと、相手synsetを引き起こす</rdfs:comment>
    <rdfs:label xml:lang="en">causes</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#is_caused_by"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#classified_by">
    <rdfs:comment xml:lang="en">Concept B is modified by classifier A when it is counted.</rdfs:comment>
    <rdfs:label xml:lang="en">classified by</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#classifies"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#classifies">
    <rdfs:comment xml:lang="en">A concept A used when counting concept B</rdfs:comment>
    <rdfs:label xml:lang="en">classifies</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#classified_by"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_agent_instrument">
    <rdfs:comment xml:lang="en">A concept which is the instrument used by a given concept in an action.</rdfs:comment>
    <rdfs:label xml:lang="en">co-agent instrument</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_instrument_agent"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_agent_patient">
    <rdfs:comment xml:lang="en">A concept which is the patient undergoing an action carried out by a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">co-agent patient</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_patient_agent"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_agent_result">
    <rdfs:comment xml:lang="en">A concept which is the result of an action taken by a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">co-agent result</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_result_agent"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_instrument_agent">
    <rdfs:comment xml:lang="en">A concept which carries out an action by using a given concept as an instrument.</rdfs:comment>
    <rdfs:label xml:lang="en">co-instrument agent</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_agent_instrument"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_instrument_patient">
    <rdfs:comment xml:lang="en">A concept which undergoes an action with the use of a given concept as an instrument.</rdfs:comment>
    <rdfs:label xml:lang="en">co-instrument patient</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_patient_instrument"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_instrument_result">
    <rdfs:comment xml:lang="en">A concept which is the result of an action using an instrument of a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">co-instrument result</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_result_instrument"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_patient_agent">
    <rdfs:comment xml:lang="en">A concept which carries out an action a given concept undergoing.</rdfs:comment>
    <rdfs:label xml:lang="en">co-patient agent</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_agent_patient"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_patient_instrument">
    <rdfs:comment xml:lang="en">A concept which is used as an instrument in an action a given concept undergoes.</rdfs:comment>
    <rdfs:label xml:lang="en">co-patient instrument</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_instrument_patient"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_result_agent">
    <rdfs:comment xml:lang="en">A concept which takes an action resulting in a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">co-result agent</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_agent_result"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_result_instrument">
    <rdfs:comment xml:lang="en">A concept which is used as an instrument in an action resulting in a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">co-result instrument</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_instrument_result"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#co_role">
    <rdfs:comment xml:lang="en">A concept undergoes an action in which a given concept is involved.</rdfs:comment>
    <rdfs:label xml:lang="en">co-role</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#co_role"/>
  </SynsetRelType>
  <owl:AnnotationProperty rdf:about="https://globalwordnet.github.io/schemas/wn#confidenceScore">
    <rdfs:comment xml:lang="en">A confidence score, usually assigned by an algorithm, with a value between 0 and 1</rdfs:comment>
    <rdfs:label xml:lang="en">confidence score</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:AnnotationProperty>
  <PartOfSpeech rdf:about="https://globalwordnet.github.io/schemas/wn#conjunction">
    <rdfs:label xml:lang="en">conjunction</rdfs:label>
    <owl:sameAs rdf:resource="http://www.lexinfo.net/ontology/3.0/lexinfo#conjunction"/>
  </PartOfSpeech>
  <owl:ObjectProperty rdf:about="https://globalwordnet.github.io/schemas/wn#count">
    <rdfs:comment xml:lang="en">The count of usages of a sense in some corpus</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
    <rdfs:label xml:lang="en">count</rdfs:label>
    <rdfs:range rdf:resource="https://globalwordnet.github.io/schemas/wn#Count"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://globalwordnet.github.io/schemas/wn#definition">
    <rdfs:comment xml:lang="en">A definition of the synset</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalConcept"/>
    <rdfs:label xml:lang="en">definition</rdfs:label>
    <rdfs:range rdf:resource="https://globalwordnet.github.io/schemas/wn#Definition"/>
  </owl:ObjectProperty>
  <SenseRelType rdf:about="https://globalwordnet.github.io/schemas/wn#derivation">
    <rdfs:comment xml:lang="en">A concept which is a derivationally related form of a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">derivation</rdfs:label>
  </SenseRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#diminutive">
    <rdfs:comment xml:lang="en">A concept used to refer to generally smaller members of a class</rdfs:comment>
    <rdfs:label xml:lang="en">diminutive</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#has_diminutive"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#direction">
    <rdfs:comment xml:lang="en">A concept which is the direction of the action or event expressed by a given concept.</rdfs:comment>
    <rdfs:label xml:lang="en">direction</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#involved_direction"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#domain_region">
    <rdf:type rdf:resource="https://globalwordnet.github.io/schemas/wn#SenseRelType"/>
    <rdfs:comment xml:lang="en">A concept which is a geographical / cultural domain pointer of a given concept.</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetが相手synsetの地域に属するものである</rdfs:comment>
    <rdfs:label xml:lang="en">domain region</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#has_domain_region"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#domain_topic">
    <rdf:type rdf:resource="https://globalwordnet.github.io/schemas/wn#SenseRelType"/>
    <rdfs:comment xml:lang="en">A concept which is the scientific category pointer of a given concept.</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetが相手synsetのカテゴリに属する</rdfs:comment>
    <rdfs:label xml:lang="en">domain topic</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#has_domain_topic"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#entails">
    <rdfs:comment xml:lang="en">impose, involve, or imply as a necessary accompaniment or result</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetを行う場合、必ず相手synsetも行っている</rdfs:comment>
    <rdfs:label xml:lang="en">entails</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#is_entailed_by"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#eq_synonym">
    <rdfs:comment xml:lang="en">X and Y are equivalent concepts but their nature requires that they remain separate (e.g. Exemplifies)</rdfs:comment>
    <rdfs:label xml:lang="en">eq synonym</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#eq_synonym"/>
  </SynsetRelType>
  <owl:ObjectProperty rdf:about="https://globalwordnet.github.io/schemas/wn#example">
    <rdfs:comment xml:lang="en">An example usage of a sense or synset</rdfs:comment>
    <rdfs:domain>
      <rdf:Description>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalConcept"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>
          </rdf:Description>
        </owl:unionOf>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:label xml:lang="en">example</rdfs:label>
    <rdfs:range rdf:resource="https://globalwordnet.github.io/schemas/wn#Example"/>
  </owl:ObjectProperty>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#exemplifies">
    <rdf:type rdf:resource="https://globalwordnet.github.io/schemas/wn#SenseRelType"/>
    <rdfs:comment xml:lang="en">A concept which is the example of a given concept.</rdfs:comment>
    <rdfs:comment xml:lang="ja">当該synsetの用法が相手synsetの領域に限られる</rdfs:comment>
    <rdfs:label xml:lang="en">exemplifies</rdfs:label>
    <owl:inverseOf rdf:resource="https://globalwordnet.github.io/schemas/wn#is_exemplified_by"/>
  </SynsetRelType>
  <SynsetRelType rdf:about="https://globalwordnet.github.io/schemas/wn#feminine">
